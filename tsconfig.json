{
  "compilerOptions": {
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "outDir": "dist",
    "removeComments": false,
    "rootDir": "app",
    "sourceMap": false,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2015",
      "dom"
    ]
  },
  "compileOnSave": false,
  "buildOnSave": false,
  "exclude": [
    ".c9",
    "dist",
    "node_modules"
  ],
  "files": [
    "./app/main.ts"
  ]
}